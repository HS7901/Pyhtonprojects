# create a loop for fruits.
fruits = [a, b, c, e, k]
for f in fruits:
    print(f)

#create a loop in range
for i in range (1,11)
print(i)

## Cratea fizz buzz game
for i in range(1,101):
    if i % 3 ==0 and i % 5 ==0:
        print("FizzBuzz")
    elif i % 3 ==0:
        print("Fizz")
    elif i % 5 ==0:
        print("Buzz")
    else:
        print(i)
